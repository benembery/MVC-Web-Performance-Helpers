@using Microsoft.Ajax.Utilities
@using WebPerformance.BundlingExtensions

@{
    ViewBag.Title = "InlineCriticalCss";
}
<section>
    <h1>Inline Critical CSS</h1>
    <p>As recommende by google</p>
    <pre class="prettyprint linenums lang-css">
@Html.Encode(@BundleHelper.RenderStylesInline("~/bundles/css"))</pre>
</section>

@section Scripts
{
    @Html.Partial("_PrettyPrintScripts")
}
